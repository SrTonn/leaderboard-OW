{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["error"] }],
    "max-len": ["error", { "code": 110 }],
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "consistent-return": "off"
  }
}
